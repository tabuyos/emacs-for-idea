<keymap version="1" name="Emacs-copy" parent="Emacs">
  <action id="$Copy">
    <keyboard-shortcut first-keystroke="ctrl insert" />
    <keyboard-shortcut first-keystroke="alt c" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="w" />
    <keyboard-shortcut first-keystroke="ctrl c" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="ctrl p" />
  </action>
  <action id="CheckinProject">
    <keyboard-shortcut first-keystroke="ctrl k" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="ctrl k" />
  </action>
  <action id="Debug">
    <keyboard-shortcut first-keystroke="shift f9" />
    <keyboard-shortcut first-keystroke="shift alt 9" />
  </action>
  <action id="Editor Copy">
    <keyboard-shortcut first-keystroke="ctrl c" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="escape" second-keystroke="y" />
  </action>
  <action id="EditorDuplicate">
    <keyboard-shortcut first-keystroke="meta d" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="d" />
  </action>
  <action id="EditorStartNewLine">
    <keyboard-shortcut first-keystroke="shift enter" />
    <keyboard-shortcut first-keystroke="ctrl enter" />
    <keyboard-shortcut first-keystroke="ctrl j" />
  </action>
  <action id="EditorTab">
    <keyboard-shortcut first-keystroke="escape" second-keystroke="tab" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="ctrl n" />
  </action>
  <action id="Generate">
    <keyboard-shortcut first-keystroke="alt insert" />
    <keyboard-shortcut first-keystroke="alt i" />
  </action>
  <action id="GotoImplementation">
    <keyboard-shortcut first-keystroke="ctrl alt b" />
    <mouse-shortcut keystroke="control alt button1" />
  </action>
  <action id="InsertLiveTemplate" />
  <action id="MoveLineDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
    <keyboard-shortcut first-keystroke="alt n" />
  </action>
  <action id="MoveLineUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
    <keyboard-shortcut first-keystroke="alt p" />
  </action>
  <action id="NewClass">
    <keyboard-shortcut first-keystroke="escape" second-keystroke="j" />
  </action>
  <action id="NewDir">
    <keyboard-shortcut first-keystroke="escape" second-keystroke="p" />
  </action>
  <action id="NewFile">
    <keyboard-shortcut first-keystroke="escape" second-keystroke="f" />
  </action>
  <action id="NewModuleInGroup">
    <keyboard-shortcut first-keystroke="escape" second-keystroke="m" />
  </action>
  <action id="RecentFiles">
    <keyboard-shortcut first-keystroke="escape" second-keystroke="e" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="r" />
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="shift f10" />
    <keyboard-shortcut first-keystroke="shift alt 0" />
  </action>
  <action id="ShowNavBar">
    <keyboard-shortcut first-keystroke="alt home" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="h" />
  </action>
  <action id="ToggleLineBreakpoint">
    <keyboard-shortcut first-keystroke="ctrl f8" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="b" />
  </action>
  <action id="Vcs.Log.GoToChild">
    <keyboard-shortcut first-keystroke="left" />
    <keyboard-shortcut first-keystroke="ctrl b" />
  </action>
  <action id="Vcs.Log.GoToParent">
    <keyboard-shortcut first-keystroke="right" />
    <keyboard-shortcut first-keystroke="ctrl f" />
  </action>
  <action id="Vcs.UpdateProject">
    <keyboard-shortcut first-keystroke="ctrl t" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="ctrl t" />
  </action>
  <action id="ViewNavigationBar">
    <keyboard-shortcut first-keystroke="escape" second-keystroke="n" />
  </action>
</keymap>